{
  "name": "board4you",
  "version": "1.0.6",
  "description": "client side of board4you",
  "main": "./public/main.js",
  "scripts": {
    "build": "./node_modules/.bin/esbuild ./src/entries/web.tsx --bundle --outdir=./public --loader:.png=dataurl --minify",
    "dev": "./node_modules/.bin/esbuild ./src/entries/web.tsx --bundle --outdir=./public --loader:.png=dataurl --watch",
    "buildMain": "./node_modules/.bin/esbuild ./src/entries/main.ts --bundle --outdir=./public --loader:.png=dataurl --platform=node",
    "devMain": "./node_modules/.bin/esbuild ./src/entries/main.ts --bundle --outdir=./public --loader:.png=dataurl --platform=node --watch",
    "electron": "electronmon .",
    "buildApp": "./node_modules/.bin/esbuild ./src/entries/web.tsx --bundle --outdir=./public --loader:.png=dataurl --minify; electron-builder -wl",
    "release": "./node_modules/.bin/esbuild ./src/entries/web.tsx --bundle --outdir=./public --loader:.png=dataurl --minify; electron-builder -wl",
    "test": "jest"
  },
  "build": {
    "fileAssociations": [
      {
        "ext": "png",
        "description": "Png File",
        "role": "Editor"
      },
      {
        "ext": "pdf",
        "name": "pdf File",
        "role": "Editor"
      },
      {
        "ext": "zip",
        "name": "Zip File",
        "role": "Editor"
      }
    ],
    "appId": "board4you",
    "publish": [
      "github"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "author": "GachiLord",
  "license": "GPL",
  "dependencies": {
    "@flaticon/flaticon-uicons": "^1.7.0",
    "@react-spring/web": "^9.6.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react": "^18.2.8",
    "@types/react-redux": "^7.1.25",
    "adm-zip": "^0.5.10",
    "bootstrap": "^5.2.3",
    "electron-updater": "^5.3.0",
    "fbemitter": "^3.0.0",
    "is-url": "^1.2.4",
    "jspdf": "^2.5.1",
    "konva": "^8.3.14",
    "pdfkit": "^0.13.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.6.0",
    "react-bootstrap-range-slider": "^3.0.8",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-konva": "^18.2.3",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.4",
    "react-router-bootstrap": "^0.26.2",
    "reconnecting-websocket": "^4.4.0",
    "semantic-ui-react": "^2.1.4",
    "tinykeys": "^2.1.0",
    "use-image": "^1.1.0",
    "use-local-storage-state": "^18.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/fbemitter": "^2.0.32",
    "@types/jest": "^29.5.3",
    "@types/pdfkit": "^0.13.1",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-bootstrap": "^0.26.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "css-loader": "^6.7.3",
    "electron": "26.4.1",
    "electron-builder": "^22.10.3",
    "electron-rebuild": "^3.2.9",
    "electronmon": "^2.0.2",
    "esbuild": "^0.19.5",
    "eslint": "^8.41.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.6.2",
    "pdfjs-dist": "^3.7.107",
    "prop-types": "^15.8.1",
    "react-router-dom": "^6.16.0",
    "typescript": "^5.2"
  }
}
