{
  "name": "board4you",
  "version": "1.0.6",
  "description": "client side of board4you",
  "main": "./src/main/main.js",
  "scripts": {
    "build": "webpack build --mode=production --no-devtool",
    "dev": "webpack -w --mode=development",
    "electron": "electronmon .",
    "start": "webpack serve --open --mode=development",
    "buildApp": "webpack build --mode=production --no-devtool; electron-builder -wl",
    "release": "webpack build --mode=production --no-devtool; electron-builder -wl",
    "test": "jest"
  },
  "build": {
    "fileAssociations": [
      {
        "ext": "png",
        "description": "Png File",
        "role": "Editor"
      },
      {
        "ext": "pdf",
        "name": "pdf File",
        "role": "Editor"
      },
      {
        "ext": "zip",
        "name": "Zip File",
        "role": "Editor"
      }
    ],
    "appId": "board4you",
    "publish": [
      "github"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "author": "GachiLord",
  "license": "GPL",
  "dependencies": {
    "@flaticon/flaticon-uicons": "^1.7.0",
    "@react-spring/web": "^9.6.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react": "^18.2.8",
    "@types/react-redux": "^7.1.25",
    "adm-zip": "^0.5.10",
    "bootstrap": "^5.2.3",
    "electron-updater": "^5.3.0",
    "fbemitter": "^3.0.0",
    "image-to-pdf": "^2.0.0",
    "is-url": "^1.2.4",
    "jspdf": "^2.5.1",
    "konva": "^8.3.14",
    "react": "^18.2.0",
    "react-bootstrap": "^2.6.0",
    "react-bootstrap-range-slider": "^3.0.8",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-konva": "^18.2.3",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.4",
    "react-router-bootstrap": "^0.26.2",
    "reconnecting-websocket": "^4.4.0",
    "semantic-ui-react": "^2.1.4",
    "tinykeys": "^2.1.0",
    "use-image": "^1.1.0",
    "use-local-storage-state": "^18.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.22.5",
    "@faker-js/faker": "^8.0.2",
    "@types/fbemitter": "^2.0.32",
    "@types/file-loader": "^5.0.2",
    "@types/jest": "^29.5.3",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-bootstrap": "^0.26.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.3",
    "electron": "^23.2.0",
    "electron-builder": "^22.10.3",
    "electron-rebuild": "^3.2.9",
    "electronmon": "^2.0.2",
    "eslint": "^8.41.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
