{
  "name": "board4you",
  "version": "1.0.7",
  "description": "client side of board4you",
  "repository": "https://github.com/GachiLord/board4you",
  "main": "./public/main.js",
  "scripts": {
    "buildWeb": "./scripts/buildWeb.sh",
    "devWeb": "./scripts/devWeb.sh",
    "buildDesktop": "./scripts/buildDesktop.sh",
    "devDesktop": "./scripts/devDesktop.sh",
    "devMain": "electron-esbuild dev -- --inspect=9229",
    "buildMain": "./scripts/buildDesktop.sh; electron-esbuild build",
    "electron": "electronmon .",
    "buildApp": "./scripts/buildApp.sh",
    "release": "./scripts/buildDesktop.sh; electron-esbuild build; electron-builder -wl",
    "test": "./scripts/test.sh",
    "grantPersmissions": "chmod +x ./scripts/*sh; chmod +x ../dev.sh"
  },
  "build": {
    "fileAssociations": [
      {
        "ext": "png",
        "description": "Png File",
        "role": "Editor"
      },
      {
        "ext": "pdf",
        "name": "pdf File",
        "role": "Editor"
      },
      {
        "ext": "zip",
        "name": "Zip File",
        "role": "Editor"
      }
    ],
    "appId": "board4you",
    "publish": [
      "github"
    ],
    "win": {
      "files": [
        "public/desktop.html",
        "public/main.js",
        "public/preload.js",
        "public/desktop.js",
        "public/desktop.css",
        "public/pdf.worker.js",
        "public/data"
      ],
      "target": "nsis"
    },
    "linux": {
      "files": [
        "public/desktop.html",
        "public/main.js",
        "public/preload.js",
        "public/desktop.js",
        "public/desktop.css",
        "public/pdf.worker.js",
        "public/data"
      ],
      "target": "AppImage"
    }
  },
  "author": "GachiLord",
  "license": "GPL",
  "dependencies": {
    "@flaticon/flaticon-uicons": "^1.7.1",
    "@react-spring/web": "^9.7.3",
    "@reacthooks.org/use-resize": "^1.0.8",
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^5.12.2",
    "@types/react": "^18.2.37",
    "@types/react-redux": "^7.1.30",
    "adm-zip": "^0.5.10",
    "bootstrap": "^5.3.2",
    "fbemitter": "^3.0.0",
    "i": "^0.3.7",
    "is-mobile": "^4.0.0",
    "is-url": "^1.2.4",
    "jest": "^29.7.0",
    "jspdf": "^2.5.1",
    "konva": "^8.4.3",
    "npm": "^10.2.3",
    "pdfkit": "^0.13.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-bootstrap-range-slider": "^3.0.8",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-konva": "^18.2.10",
    "react-redux": "^8.1.3",
    "react-router": "^6.18.0",
    "react-router-bootstrap": "^0.26.2",
    "semantic-ui-react": "^2.1.4",
    "tinykeys": "^2.1.0",
    "use-image": "^1.1.1",
    "use-local-storage-state": "^18.3.3",
    "uuid": "^9.0.1",
    "websocket-ts": "^2.1.5"
  },
  "devDependencies": {
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@faker-js/faker": "^8.2.0",
    "@types/adm-zip": "^0.5.4",
    "@types/fbemitter": "^2.0.35",
    "@types/pdfkit": "^0.13.2",
    "@types/react-color": "^3.0.10",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-bootstrap": "^0.26.6",
    "@types/uuid": "^9.0.7",
    "esbuild": "^0.19.5",
    "pdfjs-dist": "^3.11.174",
    "prop-types": "^15.8.1",
    "react-router-dom": "^6.18.0"
  },
  "optionalDependencies": {
    "electron": "26.4.1",
    "electron-builder": "^22.14.13",
    "electron-esbuild": "^8.0.0",
    "electron-updater": "^5.3.0",
    "electronmon": "^2.0.2"
  }
}
