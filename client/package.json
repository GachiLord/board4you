    "pdfjs-dist": "^3.7.107",
    "prop-types": "^15.8.1",
    "react-router-dom": "^6.16.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.3",
    "typescript": "^5.2",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  }
}
